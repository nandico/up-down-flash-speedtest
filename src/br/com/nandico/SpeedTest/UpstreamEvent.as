package br.com.nandico.SpeedTest{	import flash.events.Event;	public class UpstreamEvent extends Event	{		public static const UPSTREAM_STARTED:String = "UPSTREAM_STARTED";		public static const UPSTREAM_UPDATE:String = "UPSTREAM_UPDATE";		public static const UPSTREAM_PACKAGE_FINISHED:String = "UPSTREAM_PACKAGE_FINISHED";		public static const UPSTREAM_FINISHED:String = "UPSTREAM_FINISHED";		public static const UPSTREAM_ERROR:String = "UPSTREAM_ERROR";		public function UpstreamEvent(type:String, bubbles:Boolean=false, cancelable:Boolean=false)		{			super(type, bubbles, cancelable);		}		public override function clone():Event		{			return new UpstreamEvent(type, bubbles, cancelable);		}	}}