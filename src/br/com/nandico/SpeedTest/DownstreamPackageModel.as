package br.com.nandico.SpeedTest{	import flash.net.URLRequest;	import flash.net.URLLoader;	public class DownstreamPackageModel	{		public var url;		public var birthDate:Date;		public var deathDate:Date;		public var status:String;		public var request:URLRequest;		public var loader:URLLoader;		public var percentLoaded:Number;		public var bytesTotal:Number;		public var bytesLoaded:Number;				public static var STATUS_IDLE:String = "STATUS_IDLE";		public static var STATUS_DOWNLOADING:String = "STATUS_DOWNLOADING";		public static var STATUS_FINISHED:String = "STATUS_FINISHED";		public function DownstreamPackageModel(url)		{			this.url = url;			this.status = STATUS_IDLE;		}	}}