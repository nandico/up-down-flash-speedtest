package br.com.nandico.SpeedTest{	import flash.display.MovieClip;	import br.com.nandico.SpeedTest.ConfigLoader;	import br.com.nandico.SpeedTest.ConfigLoaderEvent;	public class SpeedTest extends MovieClip	{		public function SpeedTest()		{			ConfigLoader.addEventListener(ConfigLoaderEvent.CONFIG_LOADED, configLoadedHandler );			ConfigLoader.loadConfig();		}		private function configLoadedHandler(event:ConfigLoaderEvent):void		{			trace( "Upstream: " + event.result.upstreamUrl );			trace( "Downstream: " + event.result.downstreamUrl );		}	}}