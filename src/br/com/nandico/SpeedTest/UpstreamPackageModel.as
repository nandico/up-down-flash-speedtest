package br.com.nandico.SpeedTest{	import flash.net.URLRequest;	import flash.net.URLLoader;	import flash.net.URLVariables;	public class UpstreamPackageModel	{		public var url;		public var birthDate:Date;		public var deathDate:Date;		public var status:String;		public var request:URLRequest;		public var loader:URLLoader;		public var variables:URLVariables;		public var percentLoaded:Number;		public var desiredBytes:Number;		public var bytesTotal:Number;		public var bytesLoaded:Number;				public static var STATUS_IDLE:String = "STATUS_IDLE";		public static var STATUS_UPLOADING:String = "STATUS_UPLOADING";		public static var STATUS_FINISHED:String = "STATUS_FINISHED";		public static var STATUS_ERROR:String = "STATUS_ERROR";		public function UpstreamPackageModel(url,desiredBytes)		{			this.url = url;			this.desiredBytes = desiredBytes;			this.status = STATUS_IDLE;		}	}}